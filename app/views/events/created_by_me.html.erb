<div id="event-header" class='events-header' style='background-image: url(<%= header_background_image_url %>)'>
  <div class="container">
    <div style="text-align: center">
      <%= link_to 'Create Event', new_event_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
<div class="container-fluid events-list">
  <% @events.in_groups_of(3) do |g| %>
    <div class="row">
      <% g.compact.each do |e| %>
        <%= render partial: 'card', locals: {event: e} %>
      <% end %>
    </div>
  <% end %>
</div>